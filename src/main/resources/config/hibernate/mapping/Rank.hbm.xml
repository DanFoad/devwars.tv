<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.bezman.model.Rank" table="rank">
        <id name="id"></id>

        <property name="level" type="integer"></property>
        <property name="rank" type="string"></property>
        <property name="xpRequired" type="integer"></property>
        <property name="levelName" type="string"></property>
        <property name="rankLevel" type="integer"></property>
    </class>
</hibernate-mapping>